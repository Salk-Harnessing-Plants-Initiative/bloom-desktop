{
  "name": "bloom-desktop",
  "productName": "Bloom Desktop",
  "version": "0.1.0",
  "description": "Electron-react application for cylinder scanning and upload to bloom",
  "main": ".webpack/main",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "electron-forge start",
    "dev": "npm run build:python && electron-forge start",
    "package": "npm run build:python && electron-forge package",
    "make": "npm run build:python && electron-forge make",
    "build:python": "node scripts/build-python.js",
    "build:webpack": "npx webpack --config webpack.main.config.ts --config webpack.renderer.config.ts",
    "test:python": "uv run pytest python/tests -v --cov=python --cov-report=term --cov-report=html",
    "test:unit": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:watch": "vitest watch",
    "test:ipc": "bash scripts/ensure-python-executable.sh && ts-node tests/integration/test-ipc.ts",
    "test:streaming": "bash scripts/ensure-python-executable.sh && ts-node tests/integration/test-streaming.ts",
    "test:camera": "npm run test:streaming",
    "test:daq": "bash scripts/ensure-python-executable.sh && ts-node tests/integration/test-daq.ts",
    "test:scanner": "bash scripts/ensure-python-executable.sh && ts-node tests/integration/test-scanner.ts",
    "test:scanner-database": "bash scripts/ensure-python-executable.sh && npm run prisma:generate && ts-node tests/integration/test-scanner-database.ts",
    "test:package": "ts-node tests/integration/test-package.ts",
    "test:package:database": "bash scripts/test-package-database-full.sh",
    "test:dev:database": "bash scripts/test-dev-database.sh",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:coverage": "python3 scripts/check-ipc-coverage.py",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "seed:production": "tsx scripts/seed-production.ts",
    "studio:production": "BLOOM_DATABASE_URL=\"file:$HOME/.bloom/data/bloom.db\" prisma studio"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": {
    "name": "Salk Harnessing Plants Initiative"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.0.0",
    "dotenv": "^17.2.3",
    "electron": "28.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "happy-dom": "^20.0.5",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.6.2",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^6.18.0",
    "@types/node": "^20.11.20",
    "electron-squirrel-startup": "^1.0.0",
    "prisma": "^6.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^3.1.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  }
}

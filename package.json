{
  "name": "bloom-desktop",
  "productName": "Bloom Desktop",
  "version": "0.1.0",
  "description": "Electron-react application for cylinder scanning and upload to bloom",
  "main": ".webpack/main",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build:python": "node scripts/build-python.js",
    "test:python": "uv run pytest python/tests -v --cov=python --cov-report=term --cov-report=html",
    "test:ipc": "npm run build:python && ts-node tests/integration/test-ipc.ts",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "keywords": [],
  "author": {
    "name": "Salk Harnessing Plants Initiative"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "autoprefixer": "^10.4.17",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.0.0",
    "electron": "28.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.6.2",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@types/node": "^20.11.20",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  }
}
